#include <iostream>
using namespace std;

int main() {
    int num 1,num 2;
    char op;

    count <<"Enter two numbers"<< end 1;
    cin >> num i >> num 2;

    cout<< "Enter an operator(+,-,*,/)"<< end1;
    cin >>op;

    switch (op){
    case'+':
        count << num1 +num2;
        break;
      case '_':
         count <<num1+num2;
        break
      case'*';
        count<< num1 +num2;
        break;
      case'/':
       if(num2 !=0)
          cout<<num1 /num2;
        else
          cout<< "Error:"/ with 0 not posible!";
          break;
         default:
           cout << "invalid operator!";
            break;
           }
           return 0;
          }
